;
; BIND reverse data for {{ domain.cidr }}
;
$TTL    604800
@       IN      SOA     {{ domain.name }}. admin.{{ domain.name }}. (
                       1        ; Serial
                      604800   ; Refresh
                       86400   ; Retry
                     2419200   ; Expire
                      604800 ) ; Negative Cache TTL
;
@       IN      NS      ns.{{ domain.name }}.